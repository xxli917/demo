<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StatusBar">
        <attr name="left_image" format="reference"></attr>
        <attr name="right_image" format="reference"></attr>
        <attr name="title_text" format="reference|string"></attr>
        <attr name="title_size" format="reference|dimension"></attr>
        <attr name="title_color" format="reference|color"></attr>
        <attr name="right_text" format="reference|string"></attr>
        <attr name="right_size" format="reference|dimension"></attr>
        <attr name="right_color" format="reference|color"></attr>
        <attr name="left_visbility">
        <enum name="visible" value="0" />
        <enum name="invisible" value="1" />
        <enum name="gone" value="2" />
        </attr>
        <attr name="right_visbility">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
            <enum name="gone" value="2" />
        </attr>

    </declare-styleable>
</resources>